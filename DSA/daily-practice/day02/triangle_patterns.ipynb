    "def tringle_right_side(n):\n",
    "    for i in range(1, n+1):\n",
    "        for j in range(1, i+1):\n",
    "            print(j, end=' ')\n",
    "        print()\n",
    "\n",
    "n = 4\n",
    "tringle_right_side(n)"
